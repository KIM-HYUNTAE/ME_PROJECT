<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/edit_table"
        android:id="@+id/edit_text"
        android:gravity="right"
        android:layout_weight="1"
        android:background="@drawable/btn_round_2"
        android:textColor="#fafafa"
        android:textSize="20dp"
        android:padding="10dp"
        android:text="내 정보 수정 "
        android:layout_height="50dp"
        android:layout_width="match_parent" />

    <TableLayout
        android:id="@+id/edit_table"
        android:paddingVertical="20dp"
        app:layout_constraintTop_toBottomOf="@id/edit_text"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical">
        <TableRow
            android:layout_marginHorizontal="20dp">
            <TextView
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="이름"
                android:textColor="@color/black" />
        </TableRow>
        <TableRow
            android:gravity="center"
            android:layout_weight="1"
            android:layout_marginHorizontal="20dp">
            <EditText
                android:background="@drawable/btn_round_gray"
                android:padding="5dp"
                android:id="@+id/m_name"
                android:inputType="textPersonName"
                android:layout_weight="3" />
        </TableRow>
        <TableRow
            android:layout_marginHorizontal="20dp">
            <TextView
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="Email"
                android:textColor="@color/black"/>
        </TableRow>

        <TableRow
            android:gravity="center"
            android:layout_weight="1"
            android:layout_marginHorizontal="20dp">
            <EditText
                android:background="@drawable/btn_round_gray"
                android:padding="5dp"
                android:id="@+id/m_EmailAddress"
                android:inputType="textEmailAddress"
                android:layout_weight="2" />
            <Button
                android:id="@+id/m_email_btn"
                android:gravity="center"
                android:background="@drawable/btn_round_blue"
                android:textColor="#fefefe"
                android:text="중복확인" />
        </TableRow>
        <TableRow
            android:gravity="center"
            android:layout_marginHorizontal="20dp">
            <TextView
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="휴대폰 번호"
                android:textColor="@color/black" />
        </TableRow>
        <TableRow
            android:gravity="center"
            android:layout_weight="1"
            android:layout_marginHorizontal="20dp">
            <EditText
                android:background="@drawable/btn_round_gray"
                android:padding="5dp"
                android:id="@+id/m_editTextPhone"
                android:layout_weight="2"
                android:inputType="phone" />
            <Button
                android:id="@+id/m_phone_btn"
                android:gravity="center"
                android:background="@drawable/btn_round_blue"
                android:textColor="#fefefe"
                android:text="중복확인" />
        </TableRow>
        <TableRow
            android:gravity="center"
            android:layout_marginHorizontal="20dp">
            <TextView
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="생년월일"
            android:textColor="@color/black" />
        </TableRow>

        <TableRow
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_marginHorizontal="20dp">

            <EditText
                android:background="@drawable/btn_round_gray"
                android:padding="5dp"
                android:id="@+id/m_editTextDate"
                android:layout_weight="3"
                android:inputType="date" />
        </TableRow>
        <TableRow
            android:gravity="center"
            android:layout_marginHorizontal="20dp">
            <TextView
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="현재 비밀번호"
            android:textColor="@color/black" />
        </TableRow>

        <TableRow
            android:layout_weight="1"
            android:gravity="center"
            android:layout_marginHorizontal="20dp">

            <EditText
                android:background="@drawable/btn_round_gray"
                android:padding="5dp"
                android:layout_weight="1"
                android:id="@+id/m_TextPassword"
                android:inputType="textPassword" />
        </TableRow>
        <TableRow
            android:gravity="center"
            android:layout_marginHorizontal="20dp">
            <TextView
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="새 비밀번호"
                android:textColor="@color/black" />
        </TableRow>

        <TableRow
            android:layout_weight="1"
            android:gravity="center"
            android:layout_marginHorizontal="20dp">
            <EditText
                android:background="@drawable/btn_round_gray"
                android:padding="5dp"
                android:id="@+id/m_TextPassword2"
                android:layout_weight="1"
                android:inputType="textPassword" />
        </TableRow>
        <TableRow
            android:gravity="center"
            android:layout_marginHorizontal="20dp">
            <TextView
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:text="새 비밀번호 확인" />
        </TableRow>

        <TableRow
            android:layout_weight="1"
            android:gravity="center"
            android:layout_marginHorizontal="20dp">
            <EditText
                android:background="@drawable/btn_round_gray"
                android:padding="5dp"
                android:id="@+id/m_TextPassword3"
                android:layout_weight="1"
                android:inputType="textPassword" />
        </TableRow>
        <TableRow
            android:gravity="center"
            android:layout_marginHorizontal="20dp">
            <TextView
                android:text="MBTI"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/black" />
        </TableRow>

        <TableRow
            android:layout_weight="1"
            android:gravity="center"
            android:layout_marginHorizontal="20dp">
            <EditText
                android:background="@drawable/btn_round_gray"
                android:padding="5dp"
                android:id="@+id/m_mbti"
                android:layout_weight="3"
                android:inputType="textCapCharacters" />
        </TableRow>

        <TableRow
            android:gravity="center"
            android:layout_marginHorizontal="20dp">
            <TextView
                android:layout_weight="1"
                android:gravity="center"
                android:text="E vs I"
                android:textColor="@color/black" />
        </TableRow>

        <TableRow
            android:gravity="center"
            android:layout_marginHorizontal="10dp">

            <TextView
                android:id="@+id/m_text_E"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/black" />

            <SeekBar
                android:id="@+id/seekBar1"
                android:layout_weight="3" />

            <TextView
                android:id="@+id/m_text_I"
                android:gravity="center"
                android:text=""
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/black" />
        </TableRow>
        <TableRow
            android:gravity="center"
            android:layout_marginHorizontal="20dp">
            <TextView
                android:layout_weight="1"
                android:gravity="center"
                android:text="N vs S"
                android:textColor="@color/black" />
        </TableRow>

        <TableRow
            android:layout_gravity="center"
            android:layout_marginHorizontal="10dp">
            <TextView
                android:id="@+id/m_text_N"
                android:gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text=""
                android:textColor="@color/black" />

            <SeekBar
                android:id="@+id/seekBar2"
                android:layout_weight="3" />

            <TextView
                android:id="@+id/m_text_S"
                android:gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text=""
                android:textColor="@color/black" />
        </TableRow>
        <TableRow
            android:gravity="center"
            android:layout_marginHorizontal="20dp">
            <TextView
                android:layout_weight="1"
                android:gravity="center"
                android:text="T vs F"
                android:textColor="@color/black" />
        </TableRow>

        <TableRow
            android:gravity="center"
            android:layout_marginHorizontal="10dp">
            <TextView
                android:id="@+id/text_T"
                android:gravity="center"
                android:text=""
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/black" />

            <SeekBar
                android:id="@+id/seekBar3"
                android:layout_weight="3"/>

            <TextView
                android:id="@+id/text_F"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="@color/black" />
        </TableRow>

        <TableRow
            android:gravity="center"
            android:layout_marginHorizontal="20dp">
            <TextView
                android:gravity="center"
                android:text="P vs J"
                android:layout_weight="1"
                android:textColor="@color/black" />

        </TableRow>
        <TableRow
            android:gravity="center"
            android:layout_marginHorizontal="10dp">
            <TextView
                android:id="@+id/text_P"
                android:gravity="center"
                android:text=""
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/black" />

            <SeekBar
                android:id="@+id/seekBar4"
                android:layout_weight="3"/>

            <TextView
                android:id="@+id/text_J"
                android:layout_weight="1"
                android:gravity="center"
                android:text=""
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:textColor="@color/black" />
        </TableRow>
        <TableRow
            android:layout_weight="1"
            android:gravity="center"
            android:layout_marginHorizontal="30dp">
            <Button
                android:id="@+id/submission"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/btn_round_blue"
                android:elevation="10dp"
                android:textColor="#fefefe"
                android:text="완료" />
        </TableRow>
    </TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>